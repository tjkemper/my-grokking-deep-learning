    "input = 5.5\n",
    "weight = 0.9\n",
    "goal_pred = 10\n",
    "alpha = 0.01\n",
    "\n",
    "# Learn\n",
    "for i in range(20):\n",
    "    pred = input * weight\n",
    "    error = (pred - goal_pred) ** 2\n",
    "    derivative = (pred - goal_pred) * input\n",
    "    weight -= alpha * derivative\n",
    "    print(\"Error:\", error, \"Prediction:\", pred)\n"
